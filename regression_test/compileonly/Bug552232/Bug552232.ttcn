// actual parameters (including default values) should not produce semantic errors
// if they are not completely initialized (bug 552232)
module Bug552232 {

type record Rec {
  integer    a,
  charstring b
}
 
const Rec c_rec := { 2 }

function f_test(in Rec pl_rec := { 2 })
{
  log(pl_rec);
}

function f() {
  var Rec vl_m := { 2 }

  f_test(vl_m);
  f_test(-);
  f_test(Rec: { 2 });
  f_test(c_rec);
}

}
